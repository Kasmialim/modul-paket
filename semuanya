import alim.RumusMtk

def main():
#class volume lemari
	p = 10
	l = 9
	t = 11

	volume = alim.RumusMtk.volumeLemari(p, l, t)

	print("volume lemari")
	print("Panjang sisi\t: ", p)
	print("Lebar sisi\t: ", l)
	print("Tinggi sisi\t: ", t)
	print("Volume\t\t: ", volume)

#class luas permukaan bola
	r = 2

	luasBola = alim.RumusMtk.luaspermukaanBola(r)

	print("luas permukaan")
	print("jarijari\t: ", r)
	print("LuasPermukaan\t\t: ", luasBola)

#class luas rubik
	s = 24

	luasRubik = alim.RumusMtk.luaspermukaanRubik(s)

	print("luas permukaan")
	print("sisi\t: ", s)
	print("LuasPermukaan\t\t: ", luasRubik)

if __name__ == "__main__":
	main()


